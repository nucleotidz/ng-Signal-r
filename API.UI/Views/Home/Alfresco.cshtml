@{
    Layout = null;
}

<table class="table table-condensed">
    <thead>
        <tr>
            <th>Name</th>
            <th>Title</th>
            <th>Description</th>
            <th>URL</th>
        </tr>
    </thead>
    <tbody>

        <tr ng-repeat="data in vm.workflow">
            <td>{{data.name}}</td>
            <td>{{data.title}}</td>
            <td>{{data.description}}</td>
            <td>{{data.url}}</td>
            <td> <button class="btn btn-danger" value="Delete" ng-click="vm.Delete(data.id)">Delete</button></td>

        </tr>

    </tbody>
</table>

