
@{
    Layout = null;
}

<table class="table table-condensed">
    <thead>
        <tr>
            <th>Name</th>
            <th>Account Number</th>
            <th>Amount</th>
        </tr>
    </thead>
    <tbody>

        <tr ng-repeat="account in vm.Account">
            <td>{{account.Name}}</td>
            <td>{{account.AccountNumber}}</td>
            <td>{{account.Amount}}</td>
        </tr>
      
    </tbody>
</table>
<input type="button" value="update" class="btn btn-primary" ng-click="vm.Add()" />